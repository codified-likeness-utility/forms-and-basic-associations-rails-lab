<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.label :song_genre_id %>
  <%= f.collection_select :genre_id, @genres, :id, :name %><br>
  <input name="song[song_notes][]" id="song_notes_1">
  <input name="song[song_notes][]" id="song_notes_2">
  <input name="song[song_notes][]" id="song_notes_3">
  <br/>
  <%= f.submit %>
<% end %>
